dtd_get_categories <- function(user_token, app_id) {
categories <- POST("https://www.devtodev.com/api/v1/labels/categories/get",
body = toJSON(list("user_token" = user_token,
"app_id" = app_id),
auto_unbox = T))
content(categories)$data$categories
}
